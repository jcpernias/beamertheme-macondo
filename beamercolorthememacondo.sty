\mode<presentation>

\definecolor{macondo@dark@background}{cmyk}{0, 0, 0, 1}
\definecolor{macondo@dark@text}{cmyk}{0, 0, 0, 0.15}
\definecolor{macondo@dark@title}{cmyk}{0, 0, 0.05, 0}

\setbeamercolor{normal text}{bg=macondo@dark@background, fg=macondo@dark@text}
\setbeamercolor{alerted text}{use=normal text, fg=normal text.fg,
  bg=normal text.bg}
\setbeamercolor{structure}{fg=macondo@dark@title}

\mode
<all>
